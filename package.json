    "url": "https://github.com/andantonyan/angular2-quickstart/issues"
  },
  "homepage": "https://github.com/andantonyan/angular2-quickstart#readme",
  "dependencies": {
    "http-server": "^0.8.0",
    "tsd": "^0.6.3",
    "typescript": "^1.5.3"
  }
}
