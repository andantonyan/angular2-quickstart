{
  "name": "angular2-quickstart",
  "version": "1.0.0",
  "description": "## build ```sh $ tsc --watch -m commonjs -t es5 --emitDecoratorMetadata src/*.ts ``` ## run ```sh $ cd src && http-server ```",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/http-server",
    "watch": "./node_modules/.bin/tsc --watch -m commonjs -t es5 --emitDecoratorMetadata src/*.ts",
    "postinstall": "./node_modules/.bin/tsd install"
  },
